{
    "name": "UMDStats",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
    	"16": "icon16.png",
    	"48": "icon48.png",
    	"128": "icon128.png"
     }
	
    "content_scripts": [
        {
            "matches": ["https://app.testudo.umd.edu/soc/*"],
            "js": ["content.js"],
            "css": ["ratings.css"]
        }

    ],

    "permissions": [
      "https://api.planetterp.com/v1/*",
      "https://planetterp.com/professor/*"
    ],

    "background": {
		"scripts": ["background.js"],
		"persistent": false
    }
	


    

}
